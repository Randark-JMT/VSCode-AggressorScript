{
  "name": "aggressorscript-lsp",
  "type": "commonjs",
  "displayName": "AggressorScript Language Support",
  "description": "Language support for Cobalt Strike Aggressor Script (.cna) with syntax highlighting, code completion, and hover documentation.",
  "version": "0.1.0",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Linters"
  ],
  "keywords": [
    "aggressor",
    "aggressorscript",
    "cobalt strike",
    "cna",
    "sleep"
  ],
  "publisher": "Randark",
  "repository": {
    "type": "git",
    "url": "https://github.com/Randark-JMT/AggressorScript-lsp"
  },
  "license": "MIT",
  "main": "./out/extension.js",
  "activationEvents": [],
  "contributes": {
    "languages": [
      {
        "id": "aggressorscript",
        "aliases": [
          "AggressorScript",
          "Aggressor Script",
          "cna"
        ],
        "extensions": [
          ".cna"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./images/icon.png",
          "dark": "./images/icon.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "aggressorscript",
        "scopeName": "source.cna",
        "path": "./syntaxes/aggressorscript.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "aggressorscript",
        "path": "./snippets/aggressorscript.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.1",
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.11"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.75.0",
    "typescript": "^5.3.0"
  }
}
